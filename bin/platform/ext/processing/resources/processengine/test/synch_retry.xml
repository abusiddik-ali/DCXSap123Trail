<process xmlns="http://www.hybris.de/xsd/processdefinition" start="start" name="synchFailTestProces">

	<action id="start" bean="TestActionBean">
		<transition name="OK" to="secondStep"/>
	</action>
	
	<action id="secondStep" bean="RetryBean" canJoinPreviousNode="true">
		<transition name="OK" to="success"/>
	</action>

	<end id="success" state="SUCCEEDED">Everything was fine</end>
</process>