<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.oauth2.data.AuthenticatedUserData">
		<property name="displayName" type="java.lang.String"/>
	</bean>

	<bean class="de.hybris.ouath2.openidconnect.dto.IDTokenParameterData">
		<property name="kid" type="String"/>
		<property name="securityKeystoreLocation" type="String"/>
		<property name="securityKeystorePassword" type="String"/>
		<property name="algorithm" type="String"/>
		<property name="idTokenValiditySeconds" type="int"/>
	</bean>

</beans>
